[ Bootloader ]
  [X] init GPIO~UART
  [ ] init HDMI
  [ ] init Keyboard
  [ ] init Disk Driver
  
[ Kernel ]
  [ ] make Env Var
  [ ] make BAIO

[ Operating System ]
  [ ] make SC

-~-~-~-~-~-~-~-~-~-~

[ order ]
  1 init GPIO~UART
  2 init Disk Driver
  3 make Env Var
  4 make BAIO
  5 init HDMI
  6 init Keyboard

-~-~-~-~-~-~-~-~-~-~

[ BAIO ]
  run   < file{path} >                  ; run file
  wrt   < file{path} > ["< content >"]  ; erase content & write in file
  read  < file{path} >                  ; get file content
  chdir < dir{path} >                   ; change > here 
  del   < path >                        ; delete file | dir
  lst   [< dir{path} >]                 ; list file & dirs
  here                                  ; get > here

-~-~-~-~-~-~-~-~-~-~

[ Environnement Variable ]
  USER ; admin
  KEYB ; CH-fr
  LANG ; EN
  OSNM ; AmethystOS
  OSVR ; 0.1

-~-~-~-~-~-~-~-~-~-~

[ Basic FileSystem ]
::
  sys
    env.var  <f> * env var
    kb
      *.kb   <f> kc:ch;...
    lang
      *.lang <f> 
